    "right": {
      "button": {
        "default": "Toot",
        "conversation": "Messaggio diretto",
        "reply": "Toot di risposta",
        "deleteEdit": "Toot",
        "edit": "Toot",
        "share": "Toot"
      },
      "alert": {
        "default": {
          "title": "Pubblicazione fallita",
          "button": "Riprova"
        },
        "removeReply": {
