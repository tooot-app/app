          },
          "expiration": {
            "heading": "Durata",
            "options": {
              "300": "5 minuti",
              "1800": "30 minuti",
              "3600": "1 ora",
              "21600": "6 ore",
              "86400": "1 giorno",
              "259200": "3 giorni",
              "604800": "7 giorni",
              "cancel": "$t(common:buttons.cancel)"
            }
          }
